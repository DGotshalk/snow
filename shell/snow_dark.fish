# snow_dark fish shell theme by nightsense (https://github.com/nightsense/snow)

# normal text
set fish_color_host              b2b7bc
set fish_color_normal            b2b7bc
set fish_pager_color_completion  b2b7bc
set fish_pager_color_description b2b7bc

# muted text
set fish_color_autosuggestion    8f9599
set fish_color_comment           8f9599

# reverse muted
set fish_pager_color_progress    2b2e30 --background=#8f9599

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#565b5f
set fish_color_search_match      --background=#565b5f
set fish_color_selection         --background=#565b5f

# red errors/warnings
set fish_color_cancel            c88190
set fish_color_cwd_root          c88190
set fish_color_error             c88190

# gold special elements
set fish_color_redirection       b58d64
set fish_color_escape            b58d64
set fish_color_operator          b58d64
set fish_color_end               b58d64

# green commands/status
set fish_color_command           7f9d69
set fish_color_cwd               7f9d69
set fish_color_user              7f9d69

# cyan parameters
set fish_color_param             48a39a

# blue quoted strings
set fish_color_quote             5f9bc4

# magenta pager name match
set fish_pager_color_prefix      a58abf

